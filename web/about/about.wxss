/* web/about/about.wxss */
.container{padding: 30rpx;width:690rpx;margin-bottom: 200rpx;}
/* 切换 */
.search-nav{width:100%;height: 65rpx;margin-bottom: 30rpx;}
.search-nav .sea-li{margin-right: 40rpx;float: left;font-size: 30rpx;color: #85898d;line-height: 60rpx;position: relative;}
.search-nav .sea-current{font-size: 34rpx;color: #000;font-weight: bold;}
.search-nav .sea-current::after{position: absolute;bottom: -4rpx;content: "";width: 30rpx;height: 4rpx;border-radius: 4rpx;background: #ff2832;right: 50%;transform: translateX(15rpx);}

.main{width:100%;height: auto;}
.main .li{width:100%;height: auto;line-height: 50rpx;font-size: 30rpx;color: #000;border-bottom: 1px solid #e6e6e6;display: flex;justify-content: space-between;padding: 15rpx 0;}
.main .li .li-txt{width: 150rpx;text-align-last: justify;}
.main .li .li-txt::after{content: '：';}
.main .li .li-con{width:540rpx;}



/* 导航 */
.vtab-nav{width:100%;height: 90rpx;position: fixed;bottom: 100rpx;left: 0;z-index: 9999; color: #fff;display: flex;}
.vtab-nav .vline{width:420rpx;height: 90rpx;background: #ffa729;text-align: center;display: flex;align-items: center;flex-wrap: wrap;}
.vtab-nav .vline .top{width:100%;font-size: 32rpx;font-weight: bold;display: flex;justify-content: center;margin-top: 10rpx;align-items: center;}
.vtab-nav .vline .butt{width:100%;height: auto;font-size: 20rpx;opacity: 0.44;margin-bottom: 10rpx;}

.vtab-nav .vaddcart{width:330rpx;height: 90rpx;text-align: center;display: flex;align-items: center;flex-wrap: wrap;
/* Firefox 3.6+ */
background: -moz-linear-gradient(left, #ff2932, #ff6428); 
/* Safari 4-5, Chrome 1-9 */
/* -webkit-gradient(,  [, ]?,  [, ]? [, ]*) */
background: -webkit-gradient(linear,left,from(#ff2932),to(#ff6428));
/* Safari 5.1+, Chrome 10+ */
background: -webkit-linear-gradient(left, #ff2932, #ff6428);
/* Opera 11.10+ */
background: -o-linear-gradient(left, #ff2932, #ff6428);
}
.vtab-nav .vaddcart .top{width:100%;font-size: 32rpx;font-weight: bold;display: flex;justify-content: center;margin-top: 10rpx;align-items: center;}
.vtab-nav .vaddcart .butt{width:100%;height: auto;font-size: 20rpx;opacity: 0.44;margin-bottom: 10rpx;}

/* 企业简介 */
.abouttxt{width:100%;height: auto;font-size: 30rpx;line-height: 50rpx;color: #000;}
.abouttxt .viewmore{color:#ff2832;}

.title{width:100%;height: 100rpx;line-height: 100rpx;font-size: 34rpx;color: #000;font-weight: bold;}
.new-col{width: 690rpx;height: auto;padding: 0 30rpx;background: #fff;}
.more{float: right;font-size: 26rpx;font-weight: 300;color: #999;}
/* 企业环境 */
.pinpai-col{width:100%;height: auto;padding: 0 0;}
.pinpai-col .title{width:690rpx;height: 100rpx;line-height: 100rpx;font-size: 34rpx;color: #000;font-weight: bold;}
.pinpai-slider{width:100%;height: 368rpx;}
.pinpai-slider .pinpai-swiper{width:100%;height: 100%;}
.pinpai-slider .pinpai-swiper swiper-item{display: flex;justify-content: center;}
.pinpai-item{width:690rpx;height: 100%;position: relative;border-radius: 10rpx;overflow: hidden;}
.pinpai-item .v-img{width:100%;height: 100%;position: absolute;z-index: 1;left: 0;top: 0;border-radius: 10rpx;overflow: hidden;}
.pinpai-item .vicn{width:27rpx;height: 16rpx;position: absolute;z-index: 3;right: 0;bottom: 0;}
.more .font28{font-size: 28rpx;color: #000;}
.more .font22{font-size: 22rpx;color: #737373;}

/* 认证信息 */
.icntoysol{font-size:32rpx;color: #ff2832;font-weight: bold;background-image: url(data:image/png;base64,);background-repeat: no-repeat;background-size: 38rpx 32rpx;background-position: left center;padding-left: 50rpx;}
.icnfix{font-size: 30rpx;color: #000;background-repeat: no-repeat;background-size: 48rpx 32rpx;background-position: left center;padding-left: 55rpx;background-image: url(data:image/png;base64,);}
.icnfield{font-size: 30rpx;color: #000;background-repeat: no-repeat;background-size: 29rpx 32rpx;background-position: left center;padding-left: 40rpx;background-image: url(data:image/png;base64,);margin-left: 20rpx;}

.main .li-title{border-bottom: none;font-size: 32rpx;font-weight: bold;color: #000;line-height: 80rpx;}

.footer{margin-bottom: 100rpx;}