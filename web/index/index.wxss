/* web/index/index.wxss */

/* 搜索 */
.search{width: 750rpx;height: auto;padding: 0 30rpx 20rpx 30rpx;background: #f3f5f6;display: none;box-sizing: border-box;}
.searchcon{width:630rpx;height: 65rpx;background: #fff;padding:0 30rpx;border-radius: 64rpx;font-size: 28rpx;color: #9e9e9e;display: flex;align-items: center;justify-content: center;}
.searchcon image{width:32rpx;height: 32rpx;margin-right: 10rpx;position: relative;top: 5rpx;}
.topsearch{position: fixed;top: 0rpx;z-index: 99;display: block;}
/* 供应商信息 */
.topnav{width:710rpx;height: auto;padding: 10rpx 20rpx;background: #fff;display: flex;align-items: center;justify-content: space-between;}
.topnav .logo{width:120rpx;height: 120rpx;border-radius: 120rpx;border:1px solid #e6e6e6;box-sizing: border-box;overflow: hidden;}
.topnav .logo image{width:100%;height: 100%;}
.topnav .cen{width:510rpx;height: auto;}
.topnav .cen .comname{width:100%;height: auto;font-size: 34rpx;line-height: 54rpx;color: #000;font-weight: bold;}
.topnav .cen .font2636{width:100%;height: auto;font-size: 26rpx;line-height: 36rpx;color: #999;}
.topnav .cen .font2636 .icntoysol{padding-left:30rpx;background-image: url(data:image/png;base64,);background-repeat: no-repeat;background-size: 27rpx 23rpx; background-position: left center;margin-left: 20rpx;}

.topnav .btnShare{width: 40rpx;height: auto;padding-top: 45rpx;font-size: 20rpx;color: #ff2832;text-align: center;background-image: url(data:image/png;base64,);background-size: 40rpx 45rpx;background-repeat: no-repeat;background-position: top center;}


/* 轮播图 */
.index-swiper{width: 750rpx;height: 460rpx;}
.index-swiper image{width:750rpx;height: 460rpx;}



/* 新品上市 */
.title{width:100%;height: 100rpx;line-height: 100rpx;font-size: 34rpx;color: #000;font-weight: bold;}
.new-col{width: 690rpx;height: auto;padding: 0 30rpx;}
.more{float: right;font-size: 26rpx;font-weight: 300;color: #999;}
.more i{width:16rpx;height: 16rpx;border-radius: 16rpx;background: #e8ebec;margin-left: 10rpx;display: inline-block;}
.more .active{background: #ff2832;}

.pro-swiper{width: 100%;height:360rpx;}
.prolist{width:100%;display: flex;flex-wrap:wrap;justify-content:space-between;}
.prolist .pro{width:335rpx;background: #fff;border-radius: 10rpx;border:1px solid #e6e6e6;overflow: hidden;margin-bottom: 20rpx;position: relative;height: 360rpx;box-sizing: border-box;}
.prolist .pro .proimg{width:100%;height: 260rpx;position: relative;}
.prolist .pro .proimg .play{width:68rpx;height: 68rpx;position: absolute;bottom: 20rpx;left: 20rpx;}
.prolist .pro image{width:100%;height: 100%;float: left;}
.prolist .pro .pro-con{width:100%;height: auto;box-sizing: border-box;padding: 10rpx 25rpx;float: left;}
.prolist .pro .pro-con .font2835{width:100%;height: 45rpx;line-height: 45rpx; font-size: 28rpx;color: #000;overflow: hidden;text-overflow: ellipsis;display:inline-block;float: left;}
.prolist .pro .pro-con .font2635{width:100%;height: 35rpx;line-height: 35rpx; font-size: 26rpx;color: #999;overflow: hidden;text-overflow: ellipsis;display:inline-block;float: left;}

.prolist .pro .addicon{width:28rpx;height: 28rpx;background: #ff2832;border-radius: 28rpx;position: absolute;bottom: 15rpx;right: 20rpx;display: flex;justify-content: center;align-items: center;font-size: 28rpx;color: #fff;font-weight: 600;line-height: 28rpx;background-image: url(data:image/png;base64,);background-size: contain;background-repeat: no-repeat;}

/* commodity_screen */
.commodity_screen { width: 100%; height: 100%; position: fixed; top: 0; left: 0; background: #000; opacity: 0; overflow: hidden;  z-index: 10000;  color: #fff;}
.commodity_attr_box { width: 100%; overflow: hidden; position: fixed; bottom: 0; left: 0; z-index: 10000; background: #f5f5f5;box-shadow: 0 -1rpx 20rpx rgba(0,0,0,0.09); }
.commodity_attr_box .wx{width:375rpx;height: 150rpx;margin-bottom: 10rpx;background:#fff;text-align: center;padding: 50rpx 0;font-size: 26rpx;line-height: 50rpx;float: left;}
.commodity_attr_box .wx image{width:100rpx;height: 100rpx;border-radius: 100rpx;display: inline-block;}
.commodity_attr_box .quxiao{width:750rpx;height: 90rpx;font-size: 30rpx;line-height: 90rpx;text-align: center;background: #fff;float: left;}

.wx .shareButton{ height: 114rpx; background: none; border: 0; border-color: #FFF; font-size: 26rpx}
button::after{
border: none;
}