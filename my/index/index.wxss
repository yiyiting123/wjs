/* my/index/index.wxss */
.container{margin: 0 30rpx;width: 690rpx;}
/* 头像昵称 */
.col1{width:100%;height: auto;margin: 40rpx 0 30rpx 0;}
.col1 .headerimg{width:140rpx;height: 140rpx;border-radius: 140rpx;overflow: hidden;margin-right: 25rpx;float: left;}
.col1 .headerimg image{width:100%;height: 100%;}
.col1 .right{width:525rpx;height: auto;float: left;padding: 10rpx 0;}
.col1 .right .nickname{font-size: 40rpx;color: #000;font-weight: bold;line-height: 75rpx;}
.col1 .right .font26{font-size: 26rpx;color: #999;line-height: 46rpx;}


/* ann 公告 */
.ann{width:690rpx;height: 80rpx;box-sizing: border-box;padding: 24rpx 30rpx;background: #f3f5f6;border-radius: 10rpx;margin-bottom: 20rpx;}
.ann .left{width:90%;padding-left: 100rpx;background-image: url(data:image/png;base64,);background-repeat: no-repeat;background-size: 81rpx 31rpx;background-position: left center;font-size: 28rpx;line-height: 30rpx;float: left;box-sizing: border-box;overflow: hidden;text-overflow: ellipsis;white-space: normal;}
.ann .more{font-size: 26rpx;color: #0f73a5;float: right;}


/* col2 */
.col2{width:100%;height: auto;padding: 0 30rpx;box-sizing: border-box;border-radius: 10rpx;background: #f3f5f6;margin-bottom: 20rpx;}
.col2 .title{font-size: 32rpx;color: #000;font-weight: bold;margin: 30rpx 0;}
.col2 .con{width:100%;height: auto;display: flex;margin-bottom: 15rpx;}
.col2 .con .col_li{flex: 1;box-sizing: border-box;height: 110rpx;display: flex;justify-content: center;flex-wrap: wrap;padding: 10rpx 0;}
.col2 .con .col_li .font34{width:100%;height: auto;font-size: 34rpx;color: #000;font-weight: bold;text-align: center;line-height: 40rpx;}
.col2 .con .col_li .font28{width:100%;height: auto;font-size: 24rpx;color: #999;text-align: center;line-height: 50rpx;}
.col2 .con .hr1{width: 1px;height: 60rpx;background: #cacaca;margin: 25rpx 0;}
.col2 .con .col_li .colimg{width:50rpx;height: 50rpx;position: relative;}
.col2 .con .col_li .colimg image{width:50rpx;height: 50rpx;}
.col2 .con .col_li .redpoint{width:16rpx;height: 16rpx;border-radius: 16rpx;background: #ff2832;position: absolute;right: 0;top: 0;}

/* col3 */
.col3{width:100%;height: auto;padding: 10rpx 30rpx;box-sizing: border-box;border-radius: 10rpx;background: #f3f5f6;margin-bottom: 20rpx;}
.col3 .col3_li{width:100%;height: auto;border-bottom: 1px solid #dadada;font-size: 30rpx;line-height: 50rpx;padding: 20rpx 0;}
.col3 .col3_li:last-child{border-bottom: none;}
.col3 .col3_li .colimg{width:50rpx;height: 50rpx;position: relative;float: left;margin-right: 10rpx;}
.col3 .col3_li .colimg image{width:50rpx;height: 50rpx;}
.col3 .col3_li .redpoint{width:16rpx;height: 16rpx;border-radius: 16rpx;background: #ff2832;position: absolute;right: 0;top: 0;}
.col3 .col3_li .go{width:17rpx;height: 50rpx;float: right;}


/* 猜您喜欢 */
.new-col .title{width:100%;height: 100rpx;line-height: 100rpx;font-size: 32rpx;color: #000;font-weight: bold;}
.new-col{width: 100%;height: auto;box-sizing: border-box;}
.prolist{width:100%;display: flex;flex-wrap:wrap;justify-content:space-between;}
.prolist .pro{width:335rpx;background: #fff;border-radius: 10rpx;border: 1px solid #e6e6e6;box-sizing: border-box; overflow: hidden;margin-bottom: 20rpx;position: relative;height: 360rpx;}
.prolist .pro .proimg{width:100%;height: 260rpx;position: relative;}
.prolist .pro .proimg .play{width:68rpx;height: 68rpx;position: absolute;bottom: 20rpx;left: 20rpx;}
.prolist .pro image{width:100%;height: 100%;float: left;}
.prolist .pro .pro-con{width:100%;height: auto;box-sizing: border-box;padding: 10rpx 25rpx;float: left;}
.prolist .pro .pro-con .font2835{width:100%;height: 45rpx;line-height: 45rpx; font-size: 28rpx;color: #000;overflow: hidden;text-overflow: ellipsis;display:inline-block;float: left;}
.prolist .pro .pro-con .font2635{width:100%;height: 35rpx;line-height: 35rpx; font-size: 26rpx;color: #999;overflow: hidden;text-overflow: ellipsis;display:inline-block;float: left;}

.prolist .pro .addicon{width:28rpx;height: 28rpx;background: #ff2832;border-radius: 28rpx;position: absolute;bottom: 15rpx;right: 20rpx;display: flex;justify-content: center;align-items: center;font-size: 28rpx;color: #fff;font-weight: 600;line-height: 28rpx;background-image: url(data:image/png;base64,);background-size: contain;background-repeat: no-repeat;}

.change{width:100%;height: 100rpx;display: flex;align-items: center;justify-content: center;color: #000;font-size: 30rpx;line-height: 54rpx;}
.change image{width:29rpx;height: 33rpx;margin-right: 10rpx;}

/* my_nav */
.my_nav{width:690rpx;height: auto;padding: 40rpx 20rpx;box-sizing: border-box;display: flex;background: #f3f5f6;border-radius: 10rpx;margin-bottom: 20rpx;}
.my_nav .nav_li{flex: 1;display: flex;flex-direction: column;justify-content: space-between;align-items: center;flex-wrap: wrap;font-size: 24rpx;text-align: center;line-height: 32rpx;color: #999;}
.my_nav .nav_li image{width:79rpx;height: 79rpx;margin-bottom: 15rpx;}