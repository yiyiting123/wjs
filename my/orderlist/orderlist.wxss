/* my/orderlist/orderlist.wxss */
.container{padding: 15rpx 0 0 0;}
/* 切换 */
.search-nav{width:690rpx;height: 65rpx;margin-bottom: 18rpx;margin-left: 30rpx;margin-right: 30rpx;}
.search-nav .sea-li{margin-right: 40rpx;float: left;font-size: 30rpx;color: #85898d;line-height: 60rpx;position: relative;}
.search-nav .sea-current{font-size: 34rpx;color: #000;font-weight: bold;}
.search-nav .sea-current::after{position: absolute;bottom: -4rpx;content: "";width: 30rpx;height: 4rpx;border-radius: 4rpx;background: #ff2832;right: 50%;transform: translateX(15rpx);}

/* orderlist */
.orderlist{width: 690rpx;height: auto;margin-left: 30rpx;margin-right: 30rpx;}
.order{background: #f3f5f6;border-radius: 10rpx;margin-bottom: 20rpx;padding: 30rpx;width: 100%;box-sizing: border-box;float: left;}
.order .o-top{width:100%;height: auto;font-size: 30rpx;line-height: 40rpx;color: #000;margin-bottom: 30rpx;}
.order .o-top .o-left{padding-left: 38rpx;background-image: url(data:image/png;base64,);background-size: 31rpx 30rpx;background-position:left center;background-repeat: no-repeat;font-weight: bold;display: inline-flex;align-items: center;}
.order .o-top .o-left::after{content: '';width: 11rpx;height: 40rpx;background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAsAAAAUCAYAAABbLMdoAAAAdUlEQVQokd3RsQmAQAyF4R+udBg9J3EuNxB7XcAhdBdtbCxPAimCxx1nqQ/ShO+RIg6ogA44dbJZgADsgM9Byaq4qNAAx5tC+9NCHSkT/yhskTBx5mEh912Bk4EyQ6QScNb956BkLIWSy0C5kISSXgtyIQ2BG6+dTaiCvOwiAAAAAElFTkSuQmCC);background-size: contain;background-position: left center;background-repeat: no-repeat;margin-left: 10rpx;}
.order .o-top .o-lab{float: right;color: #ff2832;}

/* o-pros */
.o-pros{width:100%;height: auto;}
.o-pros .proimg{width:165rpx;height: 128rpx;border-radius: 10rpx;box-sizing: border-box;box-decoration-break: 1px solid #e6e6e6;overflow: hidden;display: inline-block;margin-right: 10rpx;float: left;}
.o-pros .proimg image{width: 100%;height: 100%;}
.o-pros .promore{width:68rpx;height: 128rpx;background-image: url(data:image/png;base64,);background-repeat: no-repeat;background-size: contain;background-position: left center;float: left;margin-right: 10rpx;margin-left: 25rpx;}

/* o-bott */
.o-bott{width:100%;text-align: right;margin-top: 20rpx;float: left;}
.o-bott .font26{display: inline-block;font-size: 26rpx;color: #999;line-height: 60rpx;}
.o-bott .btnDel{display: inline-block;border-radius: 60rpx;border: 1px solid #e6e6e6;box-sizing: border-box;text-align: center;line-height: 60rpx;font-size: 28rpx;color: #999;width: 160rpx;margin-left: 20rpx;}