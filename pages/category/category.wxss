/**分类栏**/
.classify{width:100%;}
.left-navbar{
  left:0;
  width:32%;
  height:calc(100% - 100rpx);
  background-color:#f9fafb;
  font-size:30rpx;
  color: #999;
  position: fixed; 
}
.left-navbar view{
  height:100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #e6e6e6;
  margin: 0 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
}
.left-navbar view:last-child{  margin-bottom: 300rpx;}
.active{
  color:#000;
  font-size:30rpx;
}
#right{
  float: right;
  width:68%;
  height:calc(100% - 100rpx);
  margin-bottom: 200rpx;
}
.cateli{min-width: 50%;height: auto;min-height: 100rpx;font-size: 30rpx;line-height: 100rpx;color: #000;float: left;padding-left: 20rpx;box-sizing: border-box;}

/* 广告区 */
.ad{width:490rpx;height: auto;margin: 10rpx;margin-bottom: 60rpx;}
.ad .ad_li{width:490rpx;height: 140rpx;border-radius: 10rpx;overflow: hidden;margin-bottom: 10rpx;}
.ad .ad_li image{width:100%;height: 100%;}